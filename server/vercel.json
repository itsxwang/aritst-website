{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "GMAIL_USER": "@gmail_user",
    "GMAIL_PASS": "@gmail_pass",
    "RECEIVER_EMAIL": "@receiver_email"
  }
}
